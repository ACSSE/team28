<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chuck.warehouse"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MicroLatitude"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.chuck.warehouse.Warehouse" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.chuck.warehouse.main" />
        <activity android:name="com.example.chuck.warehouse.login" />
        <activity android:name="com.example.chuck.warehouse.signup" />
        <activity android:name="com.example.chuck.warehouse.scanqr" />
        <activity android:name="com.example.chuck.warehouse.directmessage" />
        <activity android:name="com.example.chuck.warehouse.clients" />
        <activity android:name="com.example.chuck.warehouse.inventory" />
        <activity android:name="com.example.chuck.warehouse.notifications" />
        <activity android:name="com.example.chuck.warehouse.profile" />
        <activity
            android:name="com.example.chuck.warehouse.navigation"
            android:label="@string/title_activity_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>